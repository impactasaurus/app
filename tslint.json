{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "indent": [
      true,
      "spaces"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-var-requires": false,
    "no-empty": false,
    "ordered-imports": false,
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-console": [
      false
    ],
    "jsx-alignment": [
      false
    ],
    "max-line-length": [
      false
    ],
    "whitespace": [false],
    "jsx-no-multiline-js": false,
    "one-variable-per-declaration": false,
    "no-submodule-imports": [true, "apollo", "components", "containers", "helpers", "models", "redux", "routes", "theme", "rc-slider", "semantic-ui-less"],
    "no-implicit-dependencies": [true, "dev", ["apollo", "components", "containers", "helpers", "models", "redux", "routes", "theme"]]
  }
}
