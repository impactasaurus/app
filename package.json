{
  "name": "impactasaurus",
  "version": "0.1.0",
  "description": "The impactasaurus web app",
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "webpack --config config/webpack/index.js",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "start": "webpack-dev-server --inline --config config/webpack/index.js --host 0.0.0.0",
    "start:prod": "npm run build:prod && http-server ./build/  --proxy http://localhost:8080?",
    "test": "echo 'TODO :('",
    "lint": "eslint --fix --quiet \"src/**/**.ts*\"",
    "postinstall": "chmod +x ./src/app/theme/fixBranding.sh && ./src/app/theme/fixBranding.sh",
    "i18n-extract": "i18next 'src/**/*.tsx' -c 'src/i18n/parser.config.js'",
    "i18n-push": "crowdin push --config src/i18n/crowdin.yml",
    "i18n-pull": "crowdin pull --config src/i18n/crowdin.yml",
    "posti18n-pull": "node ./src/i18n/generate.js",
    "format": "npx prettier --write ."
  },
  "author": "Impactasaurus <admin@impactasaurus.org>",
  "homepage": "https://impactasaurus.org",
